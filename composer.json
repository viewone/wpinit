{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress",
        "type": "wordpress-core",
        "version": "3.8",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/3.8.zip"
        }
      }
    },
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "git@bitbucket.org:viewone/acf-flexible-content.git"
    },
    {
      "type": "package",
      "package": {
        "name": "acf-flexible-content",
        "type": "wordpress-plugin",
        "version": "1.1.0"
      }
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "johnpbloch/wordpress-core-installer": "~0.2",
    "composer/installers": "~1.0",
    "wordpress": "3.8",
    "wpackagist/advanced-custom-fields": ">=4.0.0",
    "viewone/acf-flexible-content": "dev-master"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}